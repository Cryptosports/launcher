{
	"name": "tivoli-cloud-vr",
	"author": "Tivoli Cloud VR, Inc.",
	"version": "0.0.19",
	"description": "Tivoli Cloud VR Launcher",
	"main": "electron/dist/main.js",
	"scripts": {
		"build:angular": "ng build --prod",
		"build:electron": "rimraf electron/dist && tsc --p electron --incremental false && npm run build:minify",
		"build:minify": "terser-folder electron/dist -e -x .js -o electron/dist --config-file terser.json",
		"build": "npm run build:angular && npm run build:electron",
		"package": "rimraf dist && electron-builder --config electron-builder.json",
		"electron": "tsc --p electron && cross-env DEV=true electron .",
		"format": "prettier --write **/*.ts **/*.json **/*.html **/*.scss",
		"start": "ng build --watch",
		"dist": "npm run build && npm run package"
	},
	"dependencies": {
		"@opentok/client": "^2.16.3",
		"discord-rpc": "^3.1.0",
		"electron-updater": "^4.2.0",
		"node-fetch": "^2.6.0",
		"socket.io-client": "^2.3.0",
		"tslib": "^1.10.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.900.0-rc.8",
		"@angular/animations": "^9.0.0-rc.8",
		"@angular/cdk": "~8.2.3",
		"@angular/cli": "~9.0.0-rc.8",
		"@angular/common": "~9.0.0-rc.8",
		"@angular/compiler": "~9.0.0-rc.8",
		"@angular/compiler-cli": "~9.0.0-rc.8",
		"@angular/core": "~9.0.0-rc.8",
		"@angular/forms": "^9.0.0-rc.8",
		"@angular/language-service": "~9.0.0-rc.8",
		"@angular/material": "^8.2.3",
		"@angular/platform-browser": "~9.0.0-rc.8",
		"@angular/platform-browser-dynamic": "~9.0.0-rc.8",
		"@angular/router": "~9.0.0-rc.8",
		"@auth0/angular-jwt": "^3.0.1",
		"@types/dom-mediacapture-record": "^1.0.2",
		"@types/electron": "^1.6.10",
		"@types/node": "^12.11.1",
		"@types/node-fetch": "^2.5.3",
		"@types/socket.io-client": "^1.4.32",
		"cross-env": "^6.0.3",
		"electron": "^7.1.1",
		"electron-builder": "^22.1.0",
		"hammerjs": "^2.0.8",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.0",
		"rxjs": "~6.5.3",
		"terser-folder": "^2.0.0",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.6.4",
		"uglify-js": "^3.7.3",
		"zone.js": "~0.10.2"
	}
}
