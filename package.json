{
	"name": "tivoli-cloud-launcher",
	"author": "Maki",
	"version": "0.0.0",
	"description": "Tivoli Cloud Launcher",
	"main": "electron/dist/main.js",
	"scripts": {
		"build": "tsc --p electron && ng build --prod",
		"dist": "electron-builder",
		"electron": "tsc --p electron && electron .",
		"format": "prettier --write **/*.ts **/*.json **/*.html **/*.scss",
		"start": "ng build --watch"
	},
	"build": {
		"appId": "com.tivolicloud.launcher",
		"asar": false,
		"win": {
			"artifactName": "tivoli-cloud-launcher.zip",
			"target": "zip",
			"compression": "normal",
			"icon": "./assets/icon.ico"
		},
		"files": [
			"out",
			"electron/dist"
		]
	},
	"dependencies": {
		"node-fetch": "^2.6.0",
		"progress-stream": "^2.0.0",
		"unzipper": "^0.10.5"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.12",
		"@angular/animations": "^8.0.0 || ^9.0.0-0",
		"@angular/cdk": "~8.2.3",
		"@angular/cli": "~8.3.12",
		"@angular/common": "~8.2.11",
		"@angular/compiler": "~8.2.11",
		"@angular/compiler-cli": "~8.2.11",
		"@angular/core": "~8.2.11",
		"@angular/forms": "^8.0.0 || ^9.0.0-0",
		"@angular/language-service": "~8.2.11",
		"@angular/material": "^8.2.3",
		"@angular/platform-browser": "~8.2.11",
		"@angular/platform-browser-dynamic": "~8.2.11",
		"@angular/router": "~8.2.11",
		"@auth0/angular-jwt": "^3.0.1",
		"@types/electron": "^1.6.10",
		"@types/node": "~8.9.4",
		"@types/node-fetch": "^2.5.3",
		"electron": "^7.1.1",
		"electron-builder": "^22.1.0",
		"hammerjs": "^2.0.8",
		"prettier": "^1.19.1",
		"rxjs": "~6.5.3",
		"ts-node": "~7.0.0",
		"tslib": "^1.10.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3",
		"zone.js": "~0.9.1"
	}
}
